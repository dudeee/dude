{
  "name": "slack-bolt",
  "description": "",
  "main": "src/index.js",
  "version": "1.5.1",
  "scripts": {
    "test": "mocha test --compilers js:babel/register -w",
    "test:single": "istanbul cover -x test _mocha -- test --compilers js:babel/register",
    "test:file": "mocha --compilers js:babel/register",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdibaiee/bolt"
  },
  "keywords": [],
  "author": "Mahdi Dibaiee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdibaiee/bolt/issues"
  },
  "files": [
    "src"
  ],
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/mdibaiee/.",
  "dependencies": {
    "agenda": "0.7.2",
    "babel": "5.8.3",
    "grunt-babel": "5.0.3",
    "mongoose": "4.2.3",
    "slackbot-api": "1.0.3"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "codecov.io": "^0.1.6",
    "ghooks": "^0.3.2",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "semantic-release": "^4.3.5"
  }
}
